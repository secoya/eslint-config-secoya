{
	"extends": "fbjs",
	"rules": {
	  "consistent-return": 0,
	  "indent": [
			"error",
			"tab",
			{
	      "SwitchCase": 1
			}
		],
	  "comma-dangle": ["error", "always-multiline"],
	  "no-var": 1,
	  "babel/new-cap": 1,
	  "babel/array-bracket-spacing": 1,
	  "babel/object-curly-spacing": 1,
	  "babel/no-await-in-loop": 1,
	  "babel/flow-object-type": 1,
	  "eqeqeq": ["error", "smart"]
	}
}
